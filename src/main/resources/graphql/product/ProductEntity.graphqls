type Product{
    id:ID
    name:String
    image:String
    price:Float
    surface:Surface
    active:Boolean
    sizes:[Sizes]
}

type PageProduct {
    content:[Product]
    totalPages:Int
    totalElements:Int
    numberOfElements: Int
}

input ProductInput{
    name:String
    image:String
    price:Float
    surface:SurfaceInput
    active:Boolean
    sizes:[Sizes]
}

